{
  "name": "vscode-rails-syntax",
  "displayName": "Rails Syntax Highlighting",
  "description": "Extends Ruby Language Grammar with Rails Library",
  "publisher": "SarahRidge",
  "icon": "images/icon.png",
  "version": "0.2.43",
  "repository": {
    "type": "git",
    "url": "https://github.com/smridge/vscode_rails_syntax"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": ["source.ruby"],
        "scopeName": "abstract_controller",
        "path": "./syntaxes/abstract_controller.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby"],
        "scopeName": "action_controller",
        "path": "./syntaxes/action_controller.tmLanguage.json"
      },
      {
        "injectTo": [ "source.ruby"],
        "scopeName": "action_dispatch",
        "path": "./syntaxes/action_dispatch.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby"],
        "scopeName": "action_text",
        "path": "./syntaxes/action_text.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby", "text.html.erb", "text.slim"],
        "scopeName": "action_view",
        "path": "./syntaxes/action_view.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby"],
        "scopeName": "active_model",
        "path": "./syntaxes/active_model.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby"],
        "scopeName": "active_record",
        "path": "./syntaxes/active_record.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby"],
        "scopeName": "active_storage",
        "path": "./syntaxes/active_storage.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby"],
        "scopeName": "module",
        "path": "./syntaxes/module.tmLanguage.json"
      },
      {
        "injectTo": ["source.ruby"],
        "scopeName": "active_support",
        "path": "./syntaxes/active_support.tmLanguage.json"
      }
    ]
  }
}
